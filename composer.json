{
    "name": "willfd/auth0middlewarepackage",
    "description": "Laravel Package for Using Auth0 Authentication in the middleware",
    "type": "library",
    "authors": [
        {
            "name": "willfd"
        }
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "^11.40",
        "auth0/auth0-php": "^8.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "willfd\\auth0middlewarepackage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
